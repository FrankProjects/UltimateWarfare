<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrankProjects\UltimateWarfare\Entity\GameNews" table="game_news">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" column="title" length="50">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="message" type="text" column="message" length="65535">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createDateTime" type="datetime" column="create_date_time"/>
    <field name="enabled" type="boolean" column="enabled"/>
    <field name="mainpage" type="boolean" column="mainpage"/>
  </entity>
</doctrine-mapping>
