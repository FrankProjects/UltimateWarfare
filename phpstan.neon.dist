parameters:
  level: 8
  paths:
    - src/
    - tests/
  bootstrapFiles:
    - vendor/bin/.phpunit/phpunit/vendor/autoload.php
  symfony:
    containerXmlPath: var/cache/dev/FrankProjects_UltimateWarfare_KernelDevDebugContainer.xml
  ignoreErrors:
    -
      message: "#^Method FrankProjects\\\\UltimateWarfare\\\\Entity\\\\User::getUserIdentifier\\(\\) should return non-empty-string but returns string\\.$#"
      count: 1
      path: src/Entity/User.php

includes:
  - vendor/phpstan/phpstan-strict-rules/rules.neon
  - vendor/phpstan/phpstan-doctrine/extension.neon
  - vendor/phpstan/phpstan-symfony/extension.neon
  - vendor/phpstan/phpstan-symfony/rules.neon
