{
    "name": "frankprojects/ultimatewarfare",
    "description": "Ultimate Warfare is a free and open-source online multiplayer browser based strategy game",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-gd": "*",
        "ext-json": "*",
        "cboden/ratchet": "^0.4",
        "doctrine/common": "^3.2",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/flex": "^1.17",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/web-profiler-bundle": "^6.0",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "^6.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/dotenv": "^6.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FrankProjects\\UltimateWarfare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FrankProjects\\UltimateWarfare\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0SF16B9AEFHYZFFXCZJK7ZN",
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
